{
  "name": "od-core-poc",
  "version": "0.0.0",
  "description": "A utility library",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "jsdom": "~15.0.0",
    "ts-jest": "^22.4.5",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "",
  "bugs": {
    "url": "http://www.overdrive.com"
  },
  "homepage": "http://www.overdrive.com",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/src/.*.test.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost"
  }
}
